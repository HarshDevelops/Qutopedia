{
    "name": "jdom",
    "version": "3.2.3",
    "description": "lightweight dom builder",
    "main": "index.js",
    "scripts": {
        "predocs": "rm -rf docs",
        "preversion": "npm run prettier",
        "prettier": "prettier --loglevel=error --write '**/*.js' '**/*.json' '**/*.jsx' '**/*.css' '**/*.md'",
        "start": "webpack-dev-server",
        "prebuild": "rm -rf ./dist",
        "build": "webpack-cli --env.NODE_ENV=production",
        "test": "mocha test"
    },
    "author": "Rob Higgins <higginsrob@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/higginsrob/jdom.git"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "devDependencies": {
        "babel-eslint": "^10.0.3",
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^6.9.0",
        "eslint-loader": "^2.2.1",
        "eslint-plugin-prettier": "^3.1.2",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^3.1.0",
        "lint-staged": "^9.5.0",
        "mocha": "^4.0.1",
        "prettier": "^1.19.1",
        "prettier-eslint": "^9.0.1",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1",
        "zombie": "^6.1.4"
    },
    "dependencies": {}
}
